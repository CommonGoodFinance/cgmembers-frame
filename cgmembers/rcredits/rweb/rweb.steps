 *     TEST Transact AMemberConfirmsRequestToChargeAnotherMember
 *     TEST Transact AMemberConfirmsRequestToPayAnotherMember
 *     TEST Transact AMemberConfirmsRequestToPayAMemberCompany
 *     TEST Transact AMemberConfirmsRequestToPayTheSameMemberTheSameAmount
 *     TEST Transact AMemberConfirmsRequestToChargeTheSameMemberTheSameAmount
 *     TEST Transact AMemberLeavesGoodsBlank
 *     TEST Transact AMemberAsksToChargeAnotherMemberBeforeTheOtherHasMadeAnRCardPurchase
 *     TEST Transact AMemberAsksToPayAnotherMemberBeforeTheOtherHasMadeAnRCardPurchase
 */
function weSayWithSubs($type, $index, $subs) {
  global $testOnly;

  $subs = @$subs[0] ?: array();
  if (isset($subs['email'])) t\fixEmail($subs['email']);
  $msg0 = u\tt($index);
  foreach ($subs as $key => $value) {
    if ($value == '?') $wild = $subs[$key] = \user_password(); // mark the wildcard spot
    if (strpos($msg0, "@$key") === FALSE) return t\output("missing sub '$key' in message '$index'");
  }
  $msg = u\tt($index, $subs);
  $msgs = \drupal_get_messages($type ?: NULL, FALSE);
  if (@$wild) {
    $i = strpos($msg, $wild);
    $tailLen = strlen($msg) - $i - strlen($wild);
    foreach ($msgs[$type] as $actual) {
      if (substr($actual, 0, $i) == substr($msg, 0, $i) and substr($actual, -$tailLen) == substr($msg, -$tailLen)) {$res = TRUE; break;}
    }
  } else {
    $tmsg = strip_tags($msg);
    foreach (@$msgs[$type] ?: [] as $actual) if (strip_tags($actual) == $tmsg) {$res = TRUE; break;}
//  $res = (@$msgs[$type] and array_search($msg, $msgs[$type]) !== FALSE);
  }
/**/  if (!@$res) debug(compact('msg', 'msgs') + ['dmsgs'=>\drupal_get_messages(NULL, FALSE)]);
  t\output("EXPECT: $msg", 'output'); // this must come after array_search!
  return @$res;
}

