/* Standard Forms */
#page-wrapper, #footer-wrapper {background: white;} /* Drupal messes this up otherwise, in Bartik theme */
#page-title {display: none;}

/* ul#main-menu-links li.first {display:none;} Use this to suppress spurious "Home" tab, but better to delete it in menu_links */

form.rweb {font-family:Arial;}
form.rweb div.invisible {display:none;}

form.rweb div#edit-title {margin-bottom:0px;}
form.rweb div#edit-title label {font-size:200%; color:darkgreen;}
form.rweb div#help-link label {font-size:80%; text-align:left; padding-left:23px; background-image:url(../../../../../images/icons/question-mark.gif); background-repeat:no-repeat; height:20px;}
form.rweb div.form-item div.description {display:block; clear:both; margin:2px 0px 0px 0px;}
form.rweb div.form-type-item label {text-align:left;}
form.rweb div.form-type-textfield {display:inline-block; margin-right:0px; vertical-align:top;}
form.rweb div.form-type-textfield label {display:inline-block;}
form.rweb div#edit-subtext label {font-weight:normal; width:100%; text-align:left;}

form.rweb input {vertical-align:top;}
form.rweb input[type="text"] {width:240px;}

form.rweb div.form-type-radios {height:25px; margin:0px;}
form.rweb div.form-radios {margin-bottom:0px;}
form.rweb div.form-type-radio {display:inline-block;}
form.rweb div.form-type-radio input {display:inline-block; margin-left:0px; vertical-align:bottom;}
form.rweb div.form-type-radio label {display:inline; width:100px; margin-left:3px; text-align:left; vertical-align:top;}

/* Labeled Forms (each field is preceded by a short label and the fields line up)
/* form.rweb.labeled (this fails for Chrome) */
form.labeled div.form-type-textfield {display:block; margin-right:0px;}
form.labeled label, form.labeled div.form-type-item label {display:inline-block; width:130px; text-align:right; margin-right:0px;} /* default label width */
form.labeled div#edit-title label,
form.labeled div#help-link,
form.labeled div.form-item div.description,
form.labeled div.description,
form.labeled input.form-submit,
form.labeled div.form-radios,
form.labeled div#edit-subtext {margin-left:135px;}

form.labeled div.description {font-size:.85em}
form.labeled div#edit-title label {text-align:left; vertical-align:top; width:100%;}
form.labeled div.labeled-block {display:inline-block; vertical-align:top;}
form.labeled div.form-textarea-wrapper {display:inline-block;}

form.labeled .form-local, form.rweb .form-local {margin:1px 2px 1px 2px; padding:2px; font-size:70%; color:darkgreen;}

form.rweb  .form-yes {color:green;}
form.rweb  .form-no {color:red;}

/* Login */
form#user-login-form input#edit-name {width:150px;}
form#user-login-form input#edit-pass {width:150px;}
#user-login-form div#edit-tip label {font-weight:normal;}

/* Register */
form#user-register-form div#edit-company-text label {font-weight:normal; width:100%;}
form#user-register-form div.form-checkboxes div {display:inline-block;}

form#user-register-form div.form-checkboxes {margin-left:135px;}
form#user-register-form div.form-type-checkbox {display:inline-block;}
form#user-register-form div.form-type-checkbox input {display:inline-block; margin-left:0px; vertical-align:bottom;}
form#user-register-form div.form-type-checkbox label {display:inline; width:100px; margin-left:3px; text-align:left; vertical-align:top;}
form#user-register-form  fieldset.form-wrapper {margin-left:0px;}
form#user-register-form  div#edit-company-text label {text-align:left;}

/* Company Info */
form#rcreditswebcompany-form fieldset#edit-description-format,
form#rcreditswebcompany-form fieldset#edit-description-format div.fieldset-wrapper {
  margin-top:0px; 
  background-color:lightblue;
}
form#rcreditswebcompany-form textarea {height:60px; width:100%; border-color:lightblue;}
form#rcreditswebcompany-form div.form-textarea-wrapper {width:796px;}
form#rcreditswebcompany-form input#edit-submit {margin-top:20px; display:inline-block;}
form#rcreditswebcompany-form div#edit-afterlinks {display:inline-block; width:400px;}
form#rcreditswebcompany-form div#edit-afterlinks label {width:100%; margin-top:20px; text-align:left;}

/* Account Info */
form#rcreditswebaccount-form div.form-type-textfield label {display:block;}
form#rcreditswebaccount-form div.form-radios {display:block;}
form#rcreditswebaccount-form div.form-item-roles {margin-top:30px;}
form#rcreditswebaccount-form div.form-item-pass-pass1 {display:inline-block;}
form#rcreditswebaccount-form div.form-item-pass-pass2 {display:inline-block; clear:right; margin-left:20px;}

/*form#rcreditswebaccount-form div.form-type-radio {display:block;}
form#rcreditswebaccount-form div.form-type-radio input {display:block;}
form#rcreditswebaccount-form div.form-type-radio label {display:block;}*/

/* Accounts Selector */
#rcreditswebaccounts-form #edit-account-submit {display:none;} /* Drupal requires unused button to be there, so make it invisible */
/* #rcreditswebaccount-form div.form-type-item {display:none;} /* no title */
#rcreditswebaccounts-form  #edit-account-submit {margin:5px 0px 0px 10px;}
#rcreditswebaccounts-form  div#edit-account-current-balance label {font-weight:normal;} /* when there is just one account */

/* Contact Info (labeled) */

/* Transact form (labeled) */
#rcreditswebtx-form.confirmation div.user-picture {float:left; margin-right:20px;}
#rcreditswebtx-form.confirmation div#id-other {float:left; clear:right; min-width:200px; max-width:300px; font-weight:normal; font-size:20px;}
#rcreditswebtx-form.confirmation div#id-name {margin-top:50px;}

/* Exchange form (labeled) */
/* #rcreditswebexchange-form div#edit-request label {font-weight:normal; text-align:left; margin-left:100px; width:100%} */
#rcreditswebexchange-form input#edit-getusd {margin-left:50px;}
#rcreditswebexchange-form div#edit-subtext {margin-left:0px; width:450px;}
#rcreditswebexchange-form div#edit-subtext div.r-explain {text-align:left; margin-left:80px;}
#rcreditswebexchange-form div#edit-subtext div.r-amounts {text-align:right; margin-right:70px;}

/* Transaction Summary Table */
#txsum, #txsum-title, #txs-title {width:550px; margin:0px;} /* outline:2px solid darkgreen;} */
#txsum-title, #txs-title {text-align:center; margin:0px; padding:0px; font-weight:bold; font-size:150%;}
#txs-title {margin-top:30px;}
#txsum td {text-align:right; border:0px;}
#txsum tr#txsum-dates {background:none; line-height:.9; border:0px;}
#txsum tr#txsum-headers {font-weight:bold; background:none; border-bottom:1px solid black;}
/*one tosign toyou fromsign fromyou rewardssign rewards balancesign balance*/
/* #txsum col.txsum-one, #txsum col.txsum-balance {width:120px;} */
#txsum col.txsum-tosign, #txsum col.txsum-fromsign, #txsum col.txsum-rewardssign, #txsum col.txsum-balancesign {width:10px;}
#txsum col.txsum-toyou, #txsum col.txsum-fromyou, #txsum col.txsum-rewards {width:75px;}

/* Transactions */
#rcreditswebtxs-form {display:inline-block; width:100%; height:45px; text-align:left;}
#rcreditswebtxs-form div.form-item-tx-period {display:inline-block;}
#rcreditswebtxs-form div.form-item-tx-task {display:inline-block;}
#rcreditswebtxs-form div.form-type-select label {display:inline-block; width:225px;} 
#rcreditswebtxs-form  input[type="submit"] {margin:5px 0px 0px 10px;}
#rcreditswebtxs-form div.txs-buttons div {display:inline-block; width:22px; text-align:center; vertical-align:middle;}
#rcreditswebtxs-form div.txs-buttons div:first-child +div input {color:red; font-weight:bold;}

#rcreditswebtxs-form col.tx-selector {width:10px;}
#rcreditswebtxs-form col.tx-tid {width:50px; text-align:right;} /* for IE; text-align in <col> should have no effect according to CSS 2.1 */ 
#rcreditswebtxs-form col.tx-date {width:60px;}
#rcreditswebtxs-form col.tx-fromyou {width:75px; text-align:right;}
#rcreditswebtxs-form col.tx-toyou {width:75px; text-align:right;}
#rcreditswebtxs-form col.tx-status {width:75px;}
#rcreditswebtxs-form col.tx-buttons {width:85px;}
#rcreditswebtxs-form col.tx-rewards {width:75px; text-align:right;}

#rcreditswebtxs-form th:first-child {text-align:right;} /* column 1 */
#rcreditswebtxs-form th:first-child +th {text-align:right;} /* column 2 */
#rcreditswebtxs-form th:first-child +th+th+th {text-align:right;} /* column 4 */
#rcreditswebtxs-form th:first-child +th+th+th+th {text-align:right;} /* column 5 */
#rcreditswebtxs-form th:first-child +th+th+th+th+th {text-align:right;} /* column 6 */
/* #rcreditswebtxs-form th:first-child +th+th+th+th+th+th {text-align:right;} /* column 7 */
#rcreditswebtxs-form th:first-child +th+th+th+th+th+th+th+th {text-align:right;} /* column 9 */

#rcreditswebtxs-form td:first-child {text-align:right;} /* column 1 */
#rcreditswebtxs-form td:first-child +td {text-align:right;} /* column 2 */
#rcreditswebtxs-form td:first-child +td+td+td {text-align:right;} /* column 4 */
#rcreditswebtxs-form td:first-child +td+td+td+td {text-align:right;} /* column 5 */
#rcreditswebtxs-form td:first-child +td+td+td+td+td {text-align:right;} /* column 6 */
/* #rcreditswebtxs-form td:first-child +td+td+td+td+td+td {text-align:right;} /* column 7 */
#rcreditswebtxs-form td:first-child +td+td+td+td+td+td+td+td {text-align:right;} /* column 9 */

/* Relation form */
/*#rcreditswebrelations-form div#edit-subtext label {width:450px;}*/

#rcreditswebrelations-form div.form-item-new-person {display:inline-block;}
#rcreditswebrelations-form div.form-item-new-person label {width:105px;}
#rcreditswebrelations-form div.form-item-new-person div.description {margin-left:110px;}
#rcreditswebrelations-form #account-label {font-weight:bold;}

/*#rcreditswebrelations-form {max-width:800px;} */
#rcreditswebrelations-form col.rel-person {width:auto;}
#rcreditswebrelations-form col.rel-employer {width:94px;}
#rcreditswebrelations-form col.rel-employee {width:96px;}
#rcreditswebrelations-form col.rel-owner{width:76px;}
#rcreditswebrelations-form col.rel-permission {width:145px;}
#rcreditswebrelations-form col.rel-link {width:200px;}
#rcreditswebrelations-form div.highlight input.form-submit {background-image:url(../../../../../images/buttons/buttons-highlight.png);}

/* Cell */
#rcreditswebcell-form input#edit-delete {margin-right:100%;}
#rcreditswebcell-form table.table-select-processed {width:300px;}

/* Example Store */
#rcreditswebexample-store-form input.form-submit, #rcreditswebexample-store-form input.form-submit:hover {background:none; border:0;}

/* Pay with rcredits */
#rcreditswebbuy-now-form div#edit-submit label {margin-left:100px; width:100%; color:blue;}

/* Directory */
#rcreditswebdirectory-form #edit-title label {margin-left:0px;}
/*#rcreditswebdirectory-form div.form-item-area {display:inline-block;} */
#rcreditswebdirectory-form input.form-submit {margin-left:5px;}
#rcreditswebdirectory-form input#edit-submit {margin-left:135px;}
/*#rcreditswebdirectory-form div.form-item-area label {width:40px;}
#rcreditswebdirectory-form div.form-item-area div.description {margin-left:45px;}
*/
#rcreditswebdirectory-form div#edit-item li {list-style-type:none;}

/* Autocomplete */
/*html.js input.form-autocomplete.throbbing {margin:0px; padding:0px; width:400px;}*/
div#autocomplete  ul {clear:none; display:inline;}
div#autocomplete  li {margin:0px; padding:0px; line-height:140%; background-color:lightyellow;}
div#content {display:block;} /* keeps autocomplete from getting placed in a wacky position */

/* Membership */
#rcreditswebmembership-form div#next-steps {width:500px; margin:-13px;}

/* Summary */
#rcreditswebsummary-form div#edit-credit-limit div.description {display:inline-block; margin-left:5px;}
#rcreditswebsummary-form div#edit-account-id {margin-bottom:0px;}
#rcreditswebsummary-form div#edit-card a {font-size:80%;}
#rcreditswebsummary-form div#edit-cardc {font-size:80%;}
#rcreditswebsummary-form div#edit-account-name {margin-bottom:0px;}

/* Profile */
#rcreditswebprofile-form {font-family:Arial;}
#rcreditswebprofile-form div.user-picture {float:left; margin-right:20px;}
#rcreditswebprofile-form div#member-details {float:left; min-width:200px; max-width:300px; font-weight:normal;}

/* Options */
#rcreditsweboptions-form  div.form-radios {margin-left:0px; display:inline-block;}
#rcreditsweboptions-form div.form-item-notify-by {margin-top:40px;}
#rcreditsweboptions-form div.form-item-secret-bal {margin-top:80px;}
#rcreditsweboptions-form div.form-item-secret-bal input {margin-left:135px;}
#rcreditsweboptions-form div.form-item-secret-bal label {margin-left:-154px; font-weight:bold;}
#rcreditsweboptions-form div.form-item-secret-bal div.description {display:inline-block; margin-left:23px;}

/* Popups */
fieldset#popup, fieldset#help {
    position: absolute;
    z-index:100;
    top: 0px;
    left: 300px;
    width: 500px;
	height: 300px;
	padding-bottom: 2px;
    background: #FFF;
    border-radius: 5px; -moz-border-radius: 5px; -webkit-border-radius: 5px;
    box-shadow: 0px 0px 4px rgba(0,0,0,0.7); -webkit-box-shadow: 0 0 4px rgba(0,0,0,0.7); -moz-box-shadow: 0 0px 4px rgba(0,0,0,0.7);
}
fieldset#popup {left:450px;} /* needs 150px extra for unknown reason */
fieldset#popup legend, fieldset#help legend {background:darkgreen; color:white;}
fieldset#popup label {text-align:left; font-weight:normal; width:100%;}
fieldset#popup select {width:480px; overflow:auto;}
fieldset#help {left:450px; height:420px; display:none;}
fieldset#help div#help-close {float:right; width:26px; height:26px; position:absolute; z-index:99; top:-12px; left:470px;}
fieldset#help div#help-close label {margin-left:0px; width:26px;}